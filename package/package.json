{
  "name": "mantine-extension-template",
  "version": "0.2.2",
  "description": "A template for Mantine extensions",
  "homepage": "https://github.com/rtivital/mantine-extension-template#readme",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "keywords": [
    "extension",
    "mantine"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": "https://github.com/mantinedev/extension-template.git",
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "bugs": "https://github.com/rtivital/extension-template/issues"
}
